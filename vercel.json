{
  "buildCommand": "cd geohints-nx && pnpm install && pnpm build",
  "outputDirectory": "geohints-nx/apps/web/.vercel/output",
  "installCommand": "cd geohints-nx && pnpm install",
  "headers": [
    {
      "source": "/build/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, s-maxage=31536000, immutable"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, s-maxage=31536000, immutable"
        }
      ]
    }
  ]
}
