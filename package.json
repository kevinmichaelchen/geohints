{
  "name": "geohints-root",
  "description": "Root workspace - see geohints-nx/ for the actual monorepo",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter @geohints-nx/source dev",
    "build": "pnpm --filter @geohints-nx/source build",
    "preview": "pnpm --filter @geohints-nx/source preview",
    "scrape": "pnpm --filter @geohints-nx/source scrape",
    "scrape:all": "pnpm --filter @geohints-nx/source scrape:all",
    "scrape:stats": "pnpm --filter @geohints-nx/source scrape:stats",
    "lint": "pnpm --filter @geohints-nx/source lint",
    "test": "pnpm --filter @geohints-nx/source test",
    "nx": "pnpm --filter @geohints-nx/source exec nx"
  }
}
